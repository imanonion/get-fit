<table class="table table-borderless" id="table-class-information">
    <tbody>
        <tr>
            <th scope="row">Day:</th>
            <td><%= startDay %></td>
        </tr>
        <tr>
            <th scope="row">Time:</th>
            <td><%= startTime %></td>
        </tr>
        <tr>
            <th scope="row">Mode</th>
            <td><%= item.mode %></td>
        </tr>
        <tr>
            <th scope="row">Type of exercise:</th>
            <td><%= item.typeOfExercise %></td>
        </tr>
        <tr>
            <th scope="row">Price:</th>
            <td>$<%= item.price %></td>
        </tr>
        <tr>
            <th scope="row">Details:</th>
            <td style="width: 10rem;" colspan="2"><%= item.details %></td>
        </tr>
        <tr>
            <td></td>
            <td><img src="<%= item.image %>"/></td>
        </tr>
        <tr>
            <th scope="row">Contact:</th>
            <td><%= item.contact %> </td>
            <td><%= item.email %></td>
        </tr>
    </tbody>
</table>